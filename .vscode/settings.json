  "chat.tools.terminal.autoApprove": {
    "python main.py < in/20_gen_06.in > result.out": {
      "approve": true,
      "matchCommandLine": true
    },
    "diff -u out/20_gen_06.out result.out | head -40": {
      "approve": true,
      "matchCommandLine": true
    },
    "python main.py < in/00_sample_00.in > result_sample.out": {
      "approve": true,
      "matchCommandLine": true
    },
    "diff": true,
    "in/20_gen_09.in": true,
    "result_20_gen_09.out": true,
    "python main.py < in/20_gen_09.in > result_20_gen_09.out": {
      "approve": true,
      "matchCommandLine": true
    },
    "in/20_gen_06.in": true,
    "result_20_gen_06.out": true,
    "in/00_sample_01.in": true,
    "result_00_sample_01.out": true,
    "set": true,
    "for": true,
    "do": true,
    "\"$f\"": true,
    "\"result_$base.out\"": true,
    "/dev/null": true,
    "}": true,
    "done": true,
    "set -e; for f in in/*.in; do base=$(basename \"$f\" .in); python main.py < \"$f\" > \"result_$base.out\"; diff -u \"out/$base.out\" \"result_$base.out\" >/dev/null || { echo \"DIFF: $base\"; diff -u \"out/$base.out\" \"result_$base.out\" | head -80; }; done": {
      "approve": true,
      "matchCommandLine": true
    },
    "/Users/daikiokayama/workspace/test/in/20_gen_06.in": true,
    "bun run build": {
      "approve": true,
      "matchCommandLine": true
    }
  },
