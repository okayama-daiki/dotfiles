{
  "editor.editContext": false,
  // workbench
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.db": "sqlite-viewer.option",
    "*.pdf": "latex-workshop-pdf-hook",
    "*.png": "luna.editor"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "workbench.activityBar.location": "bottom",
  "workbench.colorCustomizations": {
    "terminal.background": "#00000000"
  },
  // editor
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.indentation": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  // "editor.wordWrap": "on",
  // explorer
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // terminal
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.gpuAcceleration": "off", // need to transparent terminal
  // window
  "window.title": "${activeEditorLong}",
  "window.commandCenter": false,
  // files
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.git": false,
    "**/.pytest_cache": true,
    "**/.venv": true
  },
  // debug
  "debug.allowBreakpointsEverywhere": true,
  // notebook
  "notebook.output.scrolling": true,
  "notebook.lineNumbers": "on",
  "notebook.formatOnSave.enabled": true,
  "notebook.defaultFormatter": "ms-python.black-formatter",

  // Go
  // "go.goroot": "/opt/homebrew/Cellar/go/1.24.5/libexec",

  // Languages
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always",
      "source.organizeImports.ruff": "always"
    }
  },
  "flake8.enabled": false,
  "[c]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[cpp]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[cython]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ktnrg45.vscode-cython"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "[bibtex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  // Extensions
  // Pylance
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "python.analysis.typeCheckingMode": "strict",
  // flake8
  "flake8.args": ["--max-line-length=88"], // black
  // JavaScript and TypeScript Nightly
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  // Jupyter
  "jupyter.askForKernelRestart": false,
  // Markdownlint
  // Markdown PDF
  "markdown-pdf.executablePath": "/Applications/Chromium.app/Contents/MacOS/Chromium",
  // PlantUML
  "plantuml.server": "http://www.plantuml.com/plantuml",
  // Svelte
  "svelte.enable-ts-plugin": true,
  // Vibrancy Continued
  // Vim
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.vimrc",
  "vim.whichwrap": "b,s,h,l,<,>,[,],~",
  "vim.statusBarColorControl": false,
  // AREPL for Python
  "AREPL.show_to_level": 3,
  "AREPL.pythonOptions": ["-O", "-u"],
  "AREPL.customCSS": "",
  "AREPL.skipLandingPage": true,
  "AREPL.showFooter": false,
  // Trailing Spaces
  "trailing-spaces.highlightCurrentLine": false,
  // rust-analyzer
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.hover.actions.enable": true,
  "rust-analyzer.check.command": "clippy",
  // Git
  "git.confirmSync": false,
  // GitHup Copilot
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "latex": true
  },
  // Go
  "go.formatTool": "gofmt",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
